def num(x):
    if x == 0 or x==1:
        return 1
    else:
        return (x*num(x-1))
        if (x*nmu(x-1)) == 1 or (x*nmu(x-1)) == 10 or (x*nmu(x-1)) == 20 or (x*nmu(x-1)) == 30 or (x*nmu(x-1)) ==40 or (x*nmu(x-1)) == 50:
            print("not number")

