#4
class Test():
    x = [1,2,3]
a = Test()
b = Test()
a.x.append([1,2,3])
print(a.x,b.x)

c = list()
d = list()
c.append([1,2,3])

#5
class Test():
    def__init__(self):
        self.x = [1,2,3]


a = Test()
b = Test()
a.x.append([1,2,3])
print(a.x,b.x)

c = list()
d = list()
c.append([1,2,3])

#6
class parent():
    def__init__(self):
        self.p = '我是父类'

def f(self):
    print('财富')

class child(parent1,parent2,.....):
    def__init__(self):
        self.c = '我是子类'
    def f(self):
        print(self.c,'我要继承')

a = child()

a.f()
super(child.a).f()
