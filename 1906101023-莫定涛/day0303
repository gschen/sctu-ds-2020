#1
class Test():
    def original(self):
        lis = [chr(i) for i in range(97,1)]
        print(''.join(lis))
    def overturn(self):
        print(''.join([chr(i) for i in range(90,64,-1)]))

a = Test()
a.orginal()
a.overturn()

