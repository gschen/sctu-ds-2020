#单利公式

P,T,R =map(int,input(,split()))
def md(P,T,R):
    return(P*T*R)/100
print(md(P,T,R))




#找最大
print(max([14,25,98,75,23,1,4,56,59]))




#求平方和
nums [14,25,98,75,23,1,4,56,59]
n = int(input())
def sb():
    return x*x
print(sum(map(sb,nums[:n])))




#交换元素
a,b = map(int,input().split())
nums = [14,25,98,75,23,1,4,56,59]
num_a,num_b = nums[a],nums[b]
nums[b],nums[a] = num_a,num_b
print(nums)