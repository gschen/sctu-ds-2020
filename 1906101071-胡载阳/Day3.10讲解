class Person():
    def __init__(self,name,age,sex):
        self.name=name
        self.age=age
        self.sex=sex

    def peronInfo(self):
        print(self.name,self.age,self.sex)

xiaoM=Person('王小明',20,'男')
xiaoM.peronInfo()

